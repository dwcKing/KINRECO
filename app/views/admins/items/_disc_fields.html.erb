
<div class="nested-fields well well-compact">
  <div class="form-group">
    <%= f.label :disc %>
    <%= f.number_field :disc,min:1 %><br>
    <%= link_to_remove_association "ディスクの削除", f %>
  </div>

  <div class="items">
    <%= f.fields_for :songs do |song| %>
      <%= render 'song_fields', f: song %>
    <%end%>
    <div class="links">
      <%= link_to_add_association "曲の追加", f, :songs %>
    </div>
  </div>
</div>
