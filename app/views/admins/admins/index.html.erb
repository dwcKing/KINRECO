
<p>ユーザー一覧</p>

<div class="container"> <!-- bootstrap -->
<div class="table table-striped">
  <table>
    <tr>
    <td>名前</td>
    <td>住所</td>
    </tr>
    <% @endusers.each do |enduser| %>
    <tr>
      <td>
        <%= enduser.last_name %>
      </td>
      <td>
        <%= enduser.address %>
      </td>
      <td>
      <%= link_to "詳細へ",admins_admin_path(enduser.id) %>
      </td>
    </tr>
    <% end %>
    <%= paginate @endusers %>
  </table>
</div>
</div>
